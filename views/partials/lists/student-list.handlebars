{{!-- Display one list with nested tasks and complete buttons --}}
<!-- CREATE LIST -->
<div class="container">
	<div class="container #ffcdd2 red lighten-4">
    	<div class="row">
      		<form id="task-list">
        		<div class="col s6">
          			<p id="create-list" class="flow-text">{{this.title}}</p>
        		</div>
        
				{{#each this.tasks }}
       				{{> tasks/student-task}}
      			{{/each }}
       		</form>
     	</div>
  	</div>
 </div>
